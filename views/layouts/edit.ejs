<% include ../partials/header %>

<% if (msg != "") { %> <h2><%= msg %></h2> <% } %>

<form action="<%= formaction %>" method="POST">
<ul>
  <li><label for="name">name </label><input type="text" name="name" value="<%= name %>"></li>
  <li><label for="numpanes">panes </label><input type="text" name="numpanes" value="<%= numpanes %>"></li>
  <li><label for="html">html </label><textarea name="html" rows="15", cols="60"><%= html %></textarea></li>
  <li><label for="css">css </label><textarea name="css" rows="15", cols="60"><%= cssdata %></textarea></li>
  <li><button type="submit">submit</button></li>
</ul>
</form>
  
</body>
</html>